<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="356.0" prefWidth="486.0" userData="New project" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.NewProjectViewController">
   <HBox prefHeight="312.0" prefWidth="486.0">
      <children>
         <VBox prefHeight="264.0" prefWidth="146.0" spacing="10.0">
            <children>
               <Button fx:id="home" mnemonicParsing="false" onAction="#homeButtonPressed" text="Home" />
               <Button fx:id="requirements" mnemonicParsing="false" onAction="#requirementsButtonPressed" text="Requirements" />
            </children>
            <padding>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
         </VBox>
         <VBox alignment="TOP_RIGHT" prefHeight="311.0" prefWidth="353.0">
            <children>
               <HBox alignment="CENTER_RIGHT" prefHeight="50.0" prefWidth="347.0" spacing="10.0">
                  <children>
                     <Label fx:id="nameLabel" text="Name:" />
                     <TextField fx:id="projectName" prefWidth="200.0" />
                  </children>
                  <padding>
                     <Insets bottom="10.0" left="10.0" right="30.0" top="10.0" />
                  </padding>
               </HBox>
               <HBox alignment="CENTER_RIGHT" prefHeight="50.0" prefWidth="347.0" spacing="10.0">
                  <children>
                     <Label fx:id="deadlineLabel" text="Deadline: " />
                     <DatePicker fx:id="deadline" prefWidth="200.0" />
                  </children>
                  <padding>
                     <Insets bottom="10.0" left="10.0" right="30.0" top="10.0" />
                  </padding>
               </HBox>
               <HBox alignment="TOP_RIGHT" prefHeight="216.0" prefWidth="347.0" spacing="10.0">
                  <children>
                     <Label fx:id="descriptionLabel" text="Description:" />
                     <TextArea fx:id="description" prefHeight="200.0" prefWidth="220.0" />
                  </children>
                  <padding>
                     <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                  </padding>
               </HBox>
            </children>
         </VBox>
      </children>
   </HBox>
   <HBox alignment="CENTER_RIGHT" prefHeight="38.0" prefWidth="486.0">
      <children>
         <VBox prefHeight="46.0" prefWidth="344.0">
            <children>
               <Label fx:id="errorLabel" prefHeight="36.0" prefWidth="321.0" text="Error: You messed up" textFill="RED" />
            </children>
            <padding>
               <Insets bottom="10.0" left="10.0" right="10.0" />
            </padding>
         </VBox>
         <HBox alignment="CENTER_RIGHT" prefHeight="46.0" prefWidth="151.0" spacing="10.0">
            <children>
               <Button fx:id="save" mnemonicParsing="false" onAction="#saveButtonPressed" text="Save" />
               <Button fx:id="cancel" mnemonicParsing="false" onAction="#cancelButtonPressed" text="Cancel" />
            </children>
            <padding>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
         </HBox>
      </children>
   </HBox>
</VBox>
