<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="762.0" prefWidth="920.0"
      xmlns="http://javafx.com/javafx/11.0.1"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="View.ProjectViewController">
   <VBox prefHeight="815.0" prefWidth="920.0">
      <HBox prefHeight="74.0" prefWidth="816.0">
         <Label fx:id="projectLabel" alignment="CENTER" prefHeight="80.0"
                prefWidth="816.0" text="Project">
            <font>
               <Font size="24.0"/>
            </font>
         </Label>
      </HBox>
      <HBox prefHeight="147.0" prefWidth="816.0">
         <VBox prefHeight="179.0" prefWidth="408.0">
            <HBox prefHeight="50.0" prefWidth="408.0">
               <VBox.margin>
                  <Insets/>
               </VBox.margin>
               <Label prefHeight="25.0" prefWidth="75.0" text="Title:">
                  <HBox.margin>
                     <Insets left="20.0"/>
                  </HBox.margin>
                  <font>
                     <Font size="14.0"/>
                  </font>
               </Label>
               <TextField fx:id="titleText">
                  <HBox.margin>
                     <Insets left="10.0"/>
                  </HBox.margin>
               </TextField>
            </HBox>
            <HBox layoutX="10.0" layoutY="10.0" prefHeight="50.0"
                  prefWidth="408.0">
               <Label prefHeight="25.0" prefWidth="75.0" text="Customer:">
                  <HBox.margin>
                     <Insets left="20.0"/>
                  </HBox.margin>
                  <font>
                     <Font size="14.0"/>
                  </font>
               </Label>
               <TextField fx:id="customerText" onAction="#customerNameTyped"
                          prefHeight="25.0" prefWidth="150.0">
                  <HBox.margin>
                     <Insets left="10.0"/>
                  </HBox.margin>
               </TextField>
            </HBox>
            <HBox layoutX="10.0" layoutY="60.0" prefHeight="94.0"
                  prefWidth="408.0">
               <Label prefHeight="25.0" prefWidth="75.0" text="Description:">
                  <HBox.margin>
                     <Insets left="20.0"/>
                  </HBox.margin>
                  <font>
                     <Font size="14.0"/>
                  </font>
               </Label>
               <TextArea fx:id="descriptionText" prefHeight="98.0"
                         prefWidth="232.0">
                  <HBox.margin>
                     <Insets left="10.0"/>
                  </HBox.margin>
               </TextArea>
            </HBox>
         </VBox>
         <VBox layoutX="10.0" layoutY="10.0" prefHeight="122.0"
               prefWidth="408.0">
            <HBox prefHeight="49.0" prefWidth="407.0">
               <Label prefHeight="25.0" prefWidth="80.0" text="Deadline:">
                  <HBox.margin>
                     <Insets left="20.0"/>
                  </HBox.margin>
                  <font>
                     <Font size="14.0"/>
                  </font>
               </Label>
               <DatePicker fx:id="deadlineDate">
                  <HBox.margin>
                     <Insets left="10.0"/>
                  </HBox.margin>
               </DatePicker>
            </HBox>
            <HBox layoutX="10.0" layoutY="10.0" prefHeight="49.0"
                  prefWidth="407.0">
               <Label prefHeight="25.0" prefWidth="80.0" text="Progress:">
                  <HBox.margin>
                     <Insets left="20.0"/>
                  </HBox.margin>
                  <font>
                     <Font size="14.0"/>
                  </font>
               </Label>
               <TextField fx:id="progressText" editable="false">
                  <HBox.margin>
                     <Insets left="10.0"/>
                  </HBox.margin>
               </TextField>
            </HBox>
            <HBox layoutX="10.0" layoutY="60.0" prefHeight="49.0"
                  prefWidth="407.0">
               <Label prefHeight="25.0" prefWidth="80.0" text="Hours spent:">
                  <HBox.margin>
                     <Insets left="20.0"/>
                  </HBox.margin>
                  <font>
                     <Font size="14.0"/>
                  </font>
               </Label>
               <TextField fx:id="hoursSpentText" editable="false">
                  <HBox.margin>
                     <Insets left="10.0"/>
                  </HBox.margin>
               </TextField>
            </HBox>
            <HBox layoutX="10.0" layoutY="109.0" prefHeight="49.0"
                  prefWidth="407.0">
               <Label prefHeight="25.0" prefWidth="80.0" text="ID:">
                  <HBox.margin>
                     <Insets left="20.0"/>
                  </HBox.margin>
                  <font>
                     <Font size="14.0"/>
                  </font>
               </Label>
               <TextField fx:id="idText" editable="false">
                  <HBox.margin>
                     <Insets left="10.0"/>
                  </HBox.margin>
               </TextField>
            </HBox>
         </VBox>
      </HBox>
      <HBox prefHeight="58.0" prefWidth="816.0">
         <Label prefHeight="63.0" prefWidth="380.0" text="Requirements">
            <HBox.margin>
               <Insets left="20.0"/>
            </HBox.margin>
            <font>
               <Font size="18.0"/>
            </font>
         </Label>
         <Label layoutX="30.0" layoutY="10.0" prefHeight="66.0"
                prefWidth="183.0" text="Team members">
            <font>
               <Font size="18.0"/>
            </font>
            <HBox.margin>
               <Insets left="160.0"/>
            </HBox.margin>
         </Label>
      </HBox>
      <HBox prefHeight="381.0" prefWidth="920.0" spacing="10.0">
         <VBox prefHeight="342.0" prefWidth="526.0">
            <TableView fx:id="requirementListTable" prefHeight="422.0"
                       prefWidth="526.0">
               <columns>
                  <TableColumn fx:id="idCollum" prefWidth="75.0" text="ID"/>
                  <TableColumn fx:id="descriptionCollum"
                               prefWidth="217.60003662109375"
                               text="Description"/>
                  <TableColumn fx:id="priorityCollum"
                               prefWidth="120.79998779296875" text="Priority"/>
                  <TableColumn fx:id="statusCollum"
                               prefWidth="111.19998779296873" text="Status"/>
               </columns>
            </TableView>
         </VBox>
         <VBox prefHeight="342.0" prefWidth="289.0">
            <TableView fx:id="teamMemberListTable" prefHeight="462.0"
                       prefWidth="289.0">
               <columns>
                  <TableColumn fx:id="nameCollum" prefWidth="157.59992063045502"
                               text="Name"/>
                  <TableColumn fx:id="roleCollum" minWidth="7.199951171875"
                               prefWidth="132.0001220703125" text="Role"/>
               </columns>
            </TableView>
         </VBox>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
         </padding>
      </HBox>
      <VBox prefHeight="95.0" prefWidth="920.0">
         <HBox alignment="CENTER_LEFT" prefHeight="100.0"
               prefWidth="200.0" spacing="10.0">
            <Button mnemonicParsing="false" onAction="#reqDButtonPressed"
                    text="Requirement Details"/>
            <Button mnemonicParsing="false" onAction="#tMDButtonPressed"
                    text="Team member details"/>
            <Label fx:id="errorLabel" prefHeight="16.0" prefWidth="569.0"
                   text="Label" textFill="RED"/>
         </HBox>
         <HBox alignment="CENTER_LEFT" prefHeight="73.0" prefWidth="816.0"
               spacing="10.0">
            <Button mnemonicParsing="false" onAction="#addReqButtonPressed"
                    text="Add requirement">
               <HBox.margin>
                  <Insets/>
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false"
                    onAction="#removeReqButtonPressed"
                    text="Remove requirement">
               <HBox.margin>
                  <Insets/>
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#addTMButtonPressed"
                    text="Add Team member">
               <HBox.margin>
                  <Insets/>
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#removeTMButtonPressed"
                    text="Remove Team member">
               <HBox.margin>
                  <Insets/>
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#homeButtonPressed"
                    text="Home">
               <HBox.margin>
                  <Insets/>
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#backButtonPressed"
                    text="Back">
               <HBox.margin>
                  <Insets/>
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#saveButtonPressed"
                    text="Save">
               <HBox.margin>
                  <Insets/>
               </HBox.margin>
            </Button>
         </HBox>
      </VBox>
   </VBox>
</VBox>
