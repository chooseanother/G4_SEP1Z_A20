<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="239.0" prefWidth="358.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.TeamMemberViewController">
    <HBox alignment="CENTER" prefHeight="51.0" prefWidth="358.0">
        <Label fx:id="title" alignment="CENTER" text="Team Member">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
    </HBox>
    <HBox prefHeight="106.0" prefWidth="734.0">
        <VBox prefHeight="175.0" prefWidth="735.0">
            <HBox alignment="CENTER" spacing="10.0">
                <VBox.margin>
                    <Insets />
                </VBox.margin>
            <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="75.0">
               <children>
                      <Label text="Name:">
                          <HBox.margin>
                              <Insets />
                          </HBox.margin>
                          <font>
                              <Font size="14.0" />
                          </font>
                      </Label>
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
               <children>
                      <TextField fx:id="nameText" onKeyTyped="#nameTyped" prefWidth="200.0">
                          <HBox.margin>
                              <Insets />
                          </HBox.margin>
                      </TextField>
               </children>
            </HBox>
            </HBox>
            <HBox alignment="CENTER" layoutX="10.0" layoutY="10.0" spacing="10.0">
            <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="75.0">
               <children>
                      <Label text="Role:">
                          <HBox.margin>
                              <Insets />
                          </HBox.margin>
                          <font>
                              <Font size="14.0" />
                          </font>
                      </Label>
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
               <children>
                      <ChoiceBox fx:id="roleChoice" prefWidth="200.0">
                          <HBox.margin>
                              <Insets />
                          </HBox.margin>
                      </ChoiceBox>
               </children>
            </HBox>
            </HBox>
         <HBox alignment="CENTER" spacing="10.0">
            <children>
               <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="75.0">
                  <children>
                     <Label text="Id:">
                        <font>
                           <Font size="14.0" />
                        </font>
                     </Label>
                  </children>
               </HBox>
               <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
                  <children>
                     <TextField fx:id="idText" editable="false" focusTraversable="false" prefWidth="200.0" />
                  </children>
               </HBox>
            </children>
         </HBox>
        </VBox>
    </HBox>
   <Label fx:id="errorLabel" alignment="TOP_LEFT" prefHeight="34.0" prefWidth="325.0" text="Label" textFill="RED">
      <VBox.margin>
         <Insets left="20.0" />
      </VBox.margin>
   </Label>
    <HBox alignment="CENTER" prefHeight="39.0" prefWidth="358.0" spacing="10.0">
        <Button mnemonicParsing="false" onAction="#removeButtonPressed" prefHeight="26.0" text="Remove">
            <HBox.margin>
                <Insets />
            </HBox.margin>
        </Button>
        <Button mnemonicParsing="false" onAction="#saveButtonPressed" prefHeight="26.0" prefWidth="66.0" text="Save">
         <HBox.margin>
            <Insets left="50.0" />
         </HBox.margin>
        </Button>
        <Button mnemonicParsing="false" onAction="#backButtonPressed" prefHeight="26.0" prefWidth="54.0" text="Back">
            <HBox.margin>
                <Insets />
            </HBox.margin>
        </Button>
        <Button mnemonicParsing="false" onAction="#homeButtonPressed" prefHeight="26.0" text="Home">
            <HBox.margin>
                <Insets />
            </HBox.margin>
        </Button>
    </HBox>
</VBox>
