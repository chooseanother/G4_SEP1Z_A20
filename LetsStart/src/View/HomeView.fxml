<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>


<VBox xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.HomeViewController">
   <children>
      <HBox>
         <children>
            <VBox prefHeight="381.0" prefWidth="225.0">
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
               <VBox alignment="CENTER_LEFT" prefHeight="30.0" prefWidth="215.0">
                  <padding>
                     <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                  </padding>
                  <Label text="Projects" />
               </VBox>
               <VBox prefHeight="439.0" prefWidth="215.0">
                  <ListView prefHeight="380.0" prefWidth="215.0" />
               </VBox>
            </VBox>
            <VBox prefHeight="332.0" prefWidth="269.0">
               <children>
                  <VBox>
                     <children>
                        <TextField fx:id="searchProjectBar" promptText="search : ID etc." />
                     </children>
                  </VBox>
                  <HBox>
                     <children>
                        <Label text="Awesome Company">
                           <font>
                              <Font size="24.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
                  <VBox alignment="CENTER">
                     <children>
                        <Button fx:id="newProject" mnemonicParsing="false" onAction="#newProjectButtonPressed" text="New Project" />
                     </children>
                  </VBox>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</VBox>
